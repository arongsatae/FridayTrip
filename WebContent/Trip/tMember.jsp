}





//회원가입에 따른 각각의 유효성검사(글자길이, 영어 대소문자, 숫자 기타 등등...)

function checkIt()
//유효성 검사 메서드
{
	
	var form = document.forms[0];
	
	var name = document.getElementById("name");
	var id = document.getElementById("id");
	var pw = document.getElementById("pw");
	var birth = document.getElementById("birth");
	

	var addr1 = document.getElementById("addr1");
	var addr2 = document.getElementById("addr2");
	
	
	
	var tel0 = document.getElementById("tel0");
	var tel1 = document.getElementById("tel1");
	var tel2 = document.getElementById("tel2");
	
	
	var gender = document.getElementById("gender");
	var before = document.getElementById("before");
	
	

      if(name.value =="")
    	  
      {
    	  
            alert("이름를 입력하세요");
            
            form.name.focus();
            
            return false;
            
      }else{
    	  
    	  for( var i=0; i< name.value.length ; i++){
    		  
    		  
    		  ch = name.value.charAt(i);
    		  
    		  
    		  if ((ch>='0' && ch<='9') || (ch>='a' && ch<='z')) {
    	        
    	        alert ("이름은 한글만 입력가능합니다.");
    	     
    	        
    	        form.name.value="";
    	        form.name.focus();
    	        
    	        return false;
    	        
    	        }
    		   		  
    	  }
    	  
    	  
    	      	  
      }
      
      
      form.id.focus();
      
      
   
      
      if(id.value == "")
      {
            alert("아이디를 입력하세요");
            
            
            form.id.focus();
            
            return false;
            
            
            
            
      }else if(id.value == "admin"){
    	  alert("사용하실 수 없는 아이디 입니다.");
    	  return false;
      }else if(id.value.length <4 || id.value.length>=11) {
    	  
    	  alert("아이디는 4자리 이상 10자리 이하만 가능합니다.");
    	     
    	  
    	    form.id.value="";
	        form.id.focus();
	        
    	  
    	  
    	   return false;
    	   
      }else{
    	  
    	  for( var i=0; i< id.value.length ; i++){
    		  
    		  
    		  ch =id.value.charAt(i);
    		  
    		  
    		  if (!(ch>='0' && ch<='9') && !(ch>='a' && ch<='z')) {
    	        
    	        alert ("아이디는 소문자, 숫자만 입력가능합니다.");
    	     
    	        
    	 	    form.id.value="";
    	        form.id.focus();
    	        
    	        	            	        
    	        return false;
    	        
    	        }
    		   		  
    	  }
    	  
    	  
    	      	  
      }

      
     
          
      if(pw.value == "")
