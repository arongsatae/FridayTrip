    	    form.pw.value = "";
    	    form.pw.focus();
    	     
    	    
    	   return false;
      }
      
      
      
      
      form.birth.focus();
      
      
      
      if(birth.value =="")
      {
    	  
            alert("생년월일을 입력하세요");
            
            
            form.birth.focus();
            
            
            return false;
            
            
      }else if( !(birth.value.length == 6)){
    	  
    	  
           alert("생년월일은 6자리여야만 합니다. ");   	  
    	  
           
             form.birth.value= "";
             form.birth.focus(); 
           
    	    return false;
    	  
      }else{  
    	  for( var i=0; i< birth.value.length ; i++){
    		  	  
    		  ch = birth.value.charAt(i);
    		      		  
    		  if (!(ch>='0' && ch<='9')) {
    	         			  
    	        alert("생년월일은 숫자만 입력가능합니다.");
    	     
    	        
    	        form.birth.value= "";
                form.birth.focus(); 
    	        
    	        
    	        return false;
    	        
    	        
    	        }
    		  
    	  }
    	      	  
      }
      
      
      
      

      
      
      
      
      if(tel0.value == "선택")
      {
            alert("번호를 선택하세요");
            
            
            form.tel0.focus();
            
            return false;
            
      }
      
      
      
      if(tel1.value == ""){
    	  
    	  
    	    alert("전화번호를 입력하세요");
           
    	    form.tel1.focus();
    	    
            return false;
    	  
    	  
      }else {
      	
      	
     	 for( var i=0; i< tel1.value.length ; i++){
    	  	  
    		  ch = tel1.value.charAt(i);
    		      		  
    		  if (!(ch>='0' && ch<='9')) {
    	         			  
    	        alert("전호번호는 숫자만 입력할 수 있습니다.");
    	     
    	        
    	        form.tel1.value="";
    	        
    	        form.tel1.focus();
    	        
    	        return false;
    	        
    	        
    	        }
    		  
    	  }
     	
     	
     	
     	
       }
      
      
      
      
   form.tel2.focus();   
      
      
      
      if(tel2.value == ""){
    	  
    	  
  	    alert("전화번호를 입력하세요");
         
  	    
  	      form.tel2.focus();     	    
  	    
          return false;
  	  
  	  
      }else{
    		  
    		  	  
    		  for( var i=0; i< tel2.value.length ; i++){
    		  	  
        		  ch = tel2.value.charAt(i);
        		      		  
        		  if (!(ch>='0' && ch<='9')) {
        	         			  
        	        alert("전화번호는 숫자만 입력가능합니다.");
        	           	        
        	        
        	        form.tel2.value ="";   
        	        
        	        form.tel2.focus();   
        	        
        	        return false;
        	        
        	        
        	        }
        		  
        	  }
    		  		  
    	  }
    	      	  
      
      
      
     form.before.focus(); 
      
      
    
      
      
      if(before.value == "")
      {
            alert("이상형을 입력하세요");
            
            
            form.before.focus();   
            
            
            return false;
            
      }else{
		  
	  	  
		  for( var i=0; i< before.value.length ; i++){
		  	  
    		  ch = before.value.charAt(i);
    		      		  
    		  if ((ch>='0' && ch<='9')  || (ch>='a' && ch<='z') || (ch>='A' && ch<='Z')) {
    	         			
    			  
    	        alert("이상형은 한글만 입력가능합니다.");
    	     
    	        
    	        form.before.value = "";   
    	        form.before.focus();   
    	        
    	        return false;
    	        
    	        
    	        }
    		  
    	  }
		  		  
	  }
      
      
      
      
      
 
      
      
  	form.submit();
      //유효성 검사모두 통과 후 제출
      
}


</script>


